{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hammer",
      "camelcase": "Hammer",
      "title": "Hammer",
      "scope": "source.hammer",
      "file-types": ["hm"],
      "injection-regex": "^hammer$",
      "class-name": "TreeSitterHammer"
    }
  ],
  "metadata": {
    "version": "0.1.4",
    "license": "MIT",
    "description": "A tree-sitter parser for hammer",
    "authors": [
      {
        "name": "shelter-kytty",
        "email": "152751739+shelter-kytty@users.noreply.github.com",
        "url": "https://tech.lgbt/@shelter-kytty"
      }
    ],
    "links": {
      "repository": "https://github.com/shelter-kytty/tree-sitter-hammer"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
